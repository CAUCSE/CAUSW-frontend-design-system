{
  "name": "@causw/icons",
  "version": "0.0.11",
  "description": "Icon components for CAUSW Design System - CAU Software Community Service",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.cts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./mono": {
      "import": {
        "types": "./dist/mono/index.d.mts",
        "default": "./dist/mono/index.mjs"
      },
      "require": {
        "types": "./dist/mono/index.d.cts",
        "default": "./dist/mono/index.cjs"
      }
    },
    "./colored": {
      "import": {
        "types": "./dist/colored/index.d.mts",
        "default": "./dist/colored/index.mjs"
      },
      "require": {
        "types": "./dist/colored/index.d.cts",
        "default": "./dist/colored/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "generate:mono": "tsx scripts/generate.ts --mono",
    "generate:colored": "tsx scripts/generate.ts --colored",
    "prebuild": "pnpm generate",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint src",
    "test": "echo \"No tests yet\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CAUCSE/CAUSW-frontend-design-system",
    "directory": "packages/icons"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=19"
  },
  "dependencies": {
    "@causw/tokens": "workspace:*"
  },
  "devDependencies": {
    "glob": "^13.0.0"
  }
}

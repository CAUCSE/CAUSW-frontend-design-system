{
  "name": "@causw/icons",
  "version": "0.0.9",
  "description": "Icon components for CAUSW Design System - CAU Software Community Service",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./mono": {
      "types": "./dist/mono/index.d.ts",
      "import": "./dist/mono/index.mjs",
      "require": "./dist/mono/index.js"
    },
    "./colored": {
      "types": "./dist/colored/index.d.ts",
      "import": "./dist/colored/index.mjs",
      "require": "./dist/colored/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "tsx scripts/generate.ts",
    "generate:mono": "tsx scripts/generate.ts --mono",
    "generate:colored": "tsx scripts/generate.ts --colored",
    "prebuild": "pnpm generate",
    "build": "tsup src/index.ts src/mono/index.ts src/colored/index.ts --format cjs,esm --dts --external react --treeshake",
    "dev": "tsup src/index.ts src/mono/index.ts src/colored/index.ts --format cjs,esm --dts --external react --watch",
    "lint": "eslint src",
    "test": "echo \"No tests yet\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CAUCSE/CAUSW-frontend-design-system",
    "directory": "packages/icons"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": ">=19"
  },
  "devDependencies": {
    "glob": "^13.0.0"
  }
}
